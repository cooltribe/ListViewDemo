<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
    package="cyz.com.listviewdemo" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        tools:replace="android:icon"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".swipe.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".swipe.SimpleActivity"></activity>
        <activity android:name=".swipe.DifferentMenuActivity"></activity>
        <activity android:name=".sticky.MainActivity"></activity>
        <activity android:name=".pullzoom.MainActivity"></activity>
        <activity android:name=".pullzoom.PullToZoomListActivity"></activity>
        <activity android:name=".pullzoom.recyclerview.PullToZoomRecyclerActivity"></activity>
        <activity android:name=".pullzoom.PullToZoomScrollActivity"></activity>
    </application>

</manifest>
